int csd_main()
{
	unsigned* switchPointer = (unsigned*)0x41210000;
	unsigned* timerMemory = (unsigned*)001019d0;

	if(*switchPointer & 0x10000000)
	{
		*timerMemory = 1;
	}
	else if(*switchPointer & 0x01000000)
	{

		*timerMemory = 1;
	}
	else if(*switchPointer & 0x00100000)
	{
		*timerMemory = 1;
	}
	else if(*switchPointer & 0x00010000)
	{
		*timerMemory = 1;
	}
	else if(*switchPointer & 0x00001000)
	{
		*timerMemory = 1;
	}
	else if(*switchPointer & 0x00000100)
	{
		*timerMemory = 1;
	}
	else if(*switchPointer & 0x00000010)
	{
		*timerMemory = 1;
	}
	else if(*switchPointer & 0x00000001)
	{
		*timerMemory = 1;
	}
	else
	{
		*timerMemory = 1;
	}


	return 0;
}
