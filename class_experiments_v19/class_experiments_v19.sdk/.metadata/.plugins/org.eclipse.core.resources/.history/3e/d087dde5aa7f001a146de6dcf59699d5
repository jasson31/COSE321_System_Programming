
#define csd_LED_ADDR 0x41200000
#define timerMemory 0x001019d0

.extern csd_main

.global main
main:
 	ldr r0, =csd_LED_ADDR
 	ldr r1, =timerMemory
 	mov r2, #1
 	str r2, [r0]
forever:
 	//bl csd_main
	lsl r2, r2, #1
	cmp r2, #16
	bgt rotateByte

rotationReturn:
 	ldr r3, [r1]
timer:
	subs r3, r3, #1
	bne timer

 	str r2, [r0]
 	b forever

rotateByte:
	mov r2, #1
	b rotationReturn

