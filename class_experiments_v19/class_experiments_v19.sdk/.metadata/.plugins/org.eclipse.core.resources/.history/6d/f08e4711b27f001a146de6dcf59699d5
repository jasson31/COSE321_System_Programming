
#define csd_LED_ADDR 0x41200000
#define timerMemory 0x001019d0

.extern csd_main

.global main
main:
 	ldr r6, =csd_LED_ADDR
 	ldr r7, =timerMemory
 	mov r4, #1
forever:
 	str r4, [r6]
 	bl csd_main
 	ldr r5, [r7]

timer:
	subs r5, r5, #1
	bne timer

 rotation:
	lsl r4, r4, #1
	cmp r4, #0b10000000
	bgt rotateByte

 	b forever

rotateByte:
	mov r4, #1
	b timer

