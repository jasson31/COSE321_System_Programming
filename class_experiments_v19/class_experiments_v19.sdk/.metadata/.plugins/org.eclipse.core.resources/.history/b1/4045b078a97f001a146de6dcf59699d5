
#define csd_LED_ADDR 0x41200000
#define timerMemory 0x00000000

.extern csd_main

.global main
main:
 	ldr r0, =csd_LED_ADDR
 	ldr r1, =timerMemory
 	mov r2, #1
 	str r2, [r0]
forever:
	lsl r2, r2, #1
	cmp r2, #16
	bgt rotateByte

rotationReturn:
 	bl csd_main
 	ldr r3, [r1]
timer:
	subs r9, r9, #1
	bne timer
 	b forever

 	str r2, [r0]

rotateByte:
	mov r2, #1
	b rotationReturn

.data
.align 4

src:
 	.word 1, 2, 3, 4, 5, 6, 7, 8
 	.word 11, 12, 13, 14, 15, 16, 17, 18

dst:
 	.space 16	//allocate memory for 16 words
