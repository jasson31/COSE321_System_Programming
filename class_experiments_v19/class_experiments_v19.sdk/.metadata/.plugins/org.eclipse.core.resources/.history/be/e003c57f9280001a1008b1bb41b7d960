
#define csd_LED_ADDR 0x41200000
#define timerMemory 0x001019d0

.extern csd_main

.global main
main:
 	ldr r6, =csd_LED_ADDR	//Load led I/O memory address
 	ldr r7, =timerMemory	//Load timer memory address
 	mov r4, #1				//
forever:
 	str r4, [r6]
 	bl csd_main
 	ldr r5, [r7]

 rotation:
	lsl r4, r4, #1
	cmp r4, #0b10000000
	bgt rotateByte

timer:
	subs r5, r5, #1
	bne timer

 	b forever

rotateByte:
	mov r4, #1
	b timer

