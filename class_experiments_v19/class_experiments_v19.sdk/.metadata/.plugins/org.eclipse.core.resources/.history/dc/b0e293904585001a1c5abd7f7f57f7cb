int csd_main()
{
	unsigned* switchPointer = (unsigned*)0x41210000;	//Load switch I/O memory address
	unsigned* timerMemory = (unsigned*)0x001019d0;		//Load timer information memory address

	//Get switch status and set timer information depends on switch status
	if(*timerMemory == 49) { *timerMemory = 0x04333333; }
	else if(*timerMemory == 50) { *timerMemory = 0x08666666; }
	else if(*timerMemory == 51) { *timerMemory = 0x0c999999; }
	else if(*timerMemory == 52) { *timerMemory = 0x10cccccc; }
	else if(*timerMemory == 53) { *timerMemory = 0x14ffffff; }
	else if(*timerMemory == 54) { *timerMemory = 0x19333332; }
	else if(*timerMemory == 55) { *timerMemory = 0x1d666665; }
	else if(*timerMemory == 56) { *timerMemory = 0x2a000000; }

	return 0;
}
