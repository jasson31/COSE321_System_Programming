
#define csd_LED_ADDR 0x41200000
#define timerMemory 0x001019d0

.extern csd_main

.global main
main:
 	ldr r7, =timerMemory
 	ldr r6, =csd_LED_ADDR
 	mov r4, #1
 	str r4, [r6]
forever:
 	bl csd_main
 	ldr r5, [r7]

 rotation:
	lsl r4, r4, #1
	cmp r4, #16
	bgt rotateByte

timer:
	subs r5, r5, #1
	bne timer

displayLED:
 	str r4, [r0]
 	b forever

rotateByte:
	mov r4, #1
	b timer

